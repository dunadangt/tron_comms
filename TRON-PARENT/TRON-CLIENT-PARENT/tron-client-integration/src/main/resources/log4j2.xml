<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="ERROR" packages="*,com.bytesw">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n " />
        </Console>
    </Appenders>
    <Loggers>
        <Root level="DEBUG">
            <AppenderRef ref="Console" />
        </Root>
        <Logger name="javax.jmdns" additivity="true" level="TRACE">
            <AppenderRef ref="Console" />
        </Logger>
    </Loggers>
</Configuration>